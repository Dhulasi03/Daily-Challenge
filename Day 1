#include <stdio.h>
#include<math.h>

int main()
{
     int a,b;
     scanf("%d %d",&a,&b);
     char op;
     printf("enter");
     scanf(" %c",&op);
     
     
     switch(op)
     {
         case'+':
         printf("%d",a+b);
         break;
         case'-':
         printf("%d",a-b);
         case'*':
         printf("%d",a*b);
         break;
         case'/':
         if(b!=0)
         printf("%d",a/b);
         else printf("zeoi");
         break;
         default:
         printf("invalid");
     }
    
    return 0;
}
